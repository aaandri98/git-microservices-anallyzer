{
  "name": "git-microservices-analyzer",
  "displayName": "git-microservices-analyzer",
  "description": "Plugin which analyzes git metadata in order to assess microservices' systems",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-microservices-analyzer.analyze",
        "title": "Git Microservices Analyzer: perform statical analysis"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "14.x",
    "@types/vscode": "^1.56.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.1",
    "util": "^0.12.4"
  }
}
